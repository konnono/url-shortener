<div>
  <h1>URL Shortener</h1>
</div>
<div>
  <i class="fas fa-link"></i>
</div>
<div>
  <form action="/" method="POST">
    <div class="form-group">
      <input type="url" class="form-control" name="url" aria-describedby="shortenURL" placeholder="Input URL here..."
        value="http://">
    </div>
    <button type="submit" class="btn btn-primary">Shorten</button>
  </form>
</div>